{
	"name": "One Dark Pro",
	"type": "theme",
	"theme": "themes/onedarkpro.css",
	"highlight": {
		"Text": {
			"Keyword": {"Foreground": "#C678DD"},
			"Keyword.Namespace": {"Foreground": "#C678DD"},
			"Keyword.Constant": {"Foreground": "#D19A66"},
			"Keyword.Type": {"Foreground": "#C678DD"},
			"Operator": {"Foreground": "#56B6C2"},
			"Operator.Word": {"Foreground": "#C678DD"},
			"Comment": {"Foreground": "#7F848E"},
			"Comment.Multiline": {"Foreground": "#7F848E"},
			"Literal.String": {"Foreground": "#98C379"},
			"Literal.String.Double": {"Foreground": "#98C379"},
			"Literal.String.Single": {"Foreground": "#98C379"},
            "Literal.String.Escape": {"Foreground": "#56B6C2"},
			"Literal.String.Affix": {"Foreground": "#C678DD"},
			"Literal.String.Interpol": {"Foreground": "#C678DD"},
			"Literal.Number.Integer": {"Foreground": "#D19A66"},
			"Literal.Number.Float": {"Foreground": "#D19A66"},
			"Name.Class": {"Foreground": "#E5C07B"},
			"Name.Function": {"Foreground": "#61AFEF"}
		},
		"Css": {
			"Name.Namespace": {"Foreground": "#D19A66"},
			"Name.Tag": {"Foreground": "#C678DD"},
			"Name.Decorator": {"Foreground": "#56B6C2"}
		},
		"Json": {
			"Name.Tag": {"Foreground": "#E06C75"}
		},
		"Python": {
			"Name": {"Foreground": "#E06C75"},
			"Name.Namespace": {"Foreground": "#E5C07B"},
			"Name.Builtin": {"Foreground": "#61AFEF"},
			"Name.Function.Magic": {"Foreground": "#56B6C2"},
			"Name.Builtin.Pseudo": {"Foreground": "#E5C07B"},
			"Name.Exception": {"Foreground": "#E5C07B"},
			"Name.Variable.Magic": {"Foreground": "#E06C75"},
			"Punctuation": {"Foreground": "#56B6C2"}
		}, 
		"Java": {
			"Name": {"Foreground": "#E06C75"},
			"Name.Namespace": {"Foreground": "#E5C07B"},
			"Name.Declaration": {"Foreground": "#C678DD"},
			"Name.Attribute": {"Foreground": "#E5C07B"}
		},
		"Cpp": {
			"Comment.Preproc": {"Foreground": "#C678DD"},
			"Comment.PreprocFile": {"Foreground": "#98C379"},
			"Name": {"Foreground": "#E06C75"},
			"Name.Class": {"Foreground": "#E5C07B"},
			"Name.Namespace": {"Foreground": "#E5C07B"},
			"Name.Function": {"Foreground": "#61AFEF"},
			"Punctuation": {"Foreground": "#56B6C2"},
			"Operator": {"Foreground": "#C678DD"}
		}
	}
}